{
	"name": "su-translation",
	"displayName": "su-translate",
	"description": "使用翻译api将其他语言到英文,转换成常见的变量命名形式",
	"publisher": "sy",
	"icon": "",
	"version": "0.0.9",
	"engines": {
		"vscode": "^1.23.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"translate",
		"variable",
		"camel",
		"var",
		"翻译",
		"变量"
	],
	"activationEvents": [
		"onCommand:extension.varTranslation"
	],
	"main": "extension.js",
	"contributes": {
		"keybindings": [
			{
				"command": "extension.varTranslation",
				"key": "f10",
				"mac": "f10"
			}
		],
		"commands": [
			{
				"command": "extension.varTranslation",
				"title": "varTranslation",
				"when": "editorTextFocus",
				"category": "var-translation"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorTextFocus",
					"command": "extension.varTranslation"
				}
			]
		},
		"configuration": {
			"type": "object",
			"title": "varTranslation configuration",
			"properties": {
				"varTranslation.translationEngine": {
					"type": "string",
					"enum": [
						"googleCN",
						"google",
						"youdao",
						"baidu",
						"pinyin",
						"AllToEn"
					],
					"enumDescriptions": [
						"google中国",
						"google",
						"youdao",
						"baidu",
						"pinyin"
					],
					"default": "'google'",
					"description": "translationEngine"
				}
			}
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"standard-version": "^6.0.1",
		"tslint": "^5.12.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"change-case": "^3.1.0",
		"translation-api": "^1.0.5"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/xueli233/translate.git"
	},
	"homepage": "https://github.com/xueli233/translate/master/README.md",
	"__metadata": {
		"id": "",
		"publisherId": "",
		"publisherDisplayName": ""
	}
}
